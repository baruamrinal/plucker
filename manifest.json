{
  "manifest_version": 2,

  "name": "plucker",
  "description": "This extension will provide docker instance list",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "http://<your-api-domain>.com"
   ]
}
