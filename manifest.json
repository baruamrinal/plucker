{
  "manifest_version": 2,

  "name": "slocker",
  "description": "This extension will provide docker instance list",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "http://dockerv2.simplilearn.com/*"
   ]
}
